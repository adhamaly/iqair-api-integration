{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/air-quality/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/air-quality/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/air-quality",
  "projects": {
    "air-quality": {
      "type": "application",
      "root": "apps/air-quality",
      "entryFile": "main",
      "sourceRoot": "apps/air-quality/src",
      "compilerOptions": {
        "tsConfigPath": "apps/air-quality/tsconfig.app.json"
      }
    },
    "cron": {
      "type": "application",
      "root": "apps/cron",
      "entryFile": "main",
      "sourceRoot": "apps/cron/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
